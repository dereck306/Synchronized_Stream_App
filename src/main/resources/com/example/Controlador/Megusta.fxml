<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="150.0" prefWidth="236.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.Controlador.Controlador_Megusta">
   <children>
      <Label layoutX="30.0" layoutY="14.0" text="¿Te ha gustado el video?">
         <font>
            <Font name="Arial Black" size="13.0" />
         </font>
      </Label>
      <Label layoutX="33.0" layoutY="43.0" text="Digita el nombre del video">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="txtNombre" layoutX="66.0" layoutY="69.0" prefHeight="17.0" prefWidth="104.0" />
      <Button layoutX="145.0" layoutY="117.0" mnemonicParsing="false" onAction="#meGusta" text="¡Me Gusta!" />
      <Button fx:id="volverLike" layoutX="14.0" layoutY="117.0" mnemonicParsing="false" onAction="#switchToSceneMediaPlayer" />
   </children>
</AnchorPane>
