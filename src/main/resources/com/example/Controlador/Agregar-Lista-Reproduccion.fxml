      <Label layoutX="2.0" layoutY="14.0" text="Deseas Agregarlo a la lista de Reproduccion">
         <font>
            <Font name="Arial Black" size="13.0" />
         </font>
      </Label>
      <Button fx:id="buttonYes" layoutX="173.0" layoutY="64.0" mnemonicParsing="false" onAction="#switchToSceneAgregarNombreListaID" text="Si">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="buttonNo" layoutX="85.0" layoutY="64.0" mnemonicParsing="false" onAction="#switchToSceneAgregarVideo" text="No">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
